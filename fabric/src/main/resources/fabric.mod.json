  "name": "Patternmaster's Fix",
  "description": "A simple mod that makes Patternmaster's Purification stop erroring when used on non-static patterns.",
  "authors": [
    "beholderface"
  ],
  "contact": {
    "homepage": "https://github.com/beholderface/Patternmasters-Fix",
    "sources": "https://github.com/beholderface/Patternmasters-Fix"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.beholderface.patternmastersfix.fabric.PatternmastersFixFabric"
    ],
    "client": [
      "net.beholderface.patternmastersfix.fabric.client.PatternmastersFixFabricClient"
    ]
  },
  "mixins": [
    "patternmasters-fix.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*",
    "hexcasting": ">=0.11.2",
    "moreiotas": ">=0.1.1"
  }
